<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <appSettings>
    <!--path in string-->
    <add key="SoundLocationPath" value="Alarm Clock Sound.wav" />
    <!--milli seconds.default one second-->
    <add key="SoundPlayLength" value="1000" />
    <!--milli seconds. default for 60seconds-->
    <add key="TimerElapseTime" value="60000" />
  </appSettings>
</configuration>